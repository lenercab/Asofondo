<!-- Cunsulta nombre completo del vinculado, numero documento, numero radicado, fecha vinculacion y el estado del vinculado -->
		<tns:MetadataQuery>
			<tns:QueryName>SP_VinculadoCertificado</tns:QueryName>
			<tns:CommandText>BEPS_TRANSACCIONAL.vinculacion.pr_vinc_consultarCertificadoVinculado(?,?,?)</tns:CommandText>
				<tns:Parameters>
					<tns:Parameter>
						<tns:ParameterName>@tipoDocumento</tns:ParameterName>
						<tns:ParameterType>VARCHAR</tns:ParameterType>
						<tns:ParameterScale>2</tns:ParameterScale>
					</tns:Parameter>
					<tns:Parameter>
						<tns:ParameterName>@numDocumento</tns:ParameterName>
						<tns:ParameterType>VARCHAR</tns:ParameterType>
						<tns:ParameterScale>15</tns:ParameterScale>
					</tns:Parameter>
					<tns:Parameter>
						<tns:ParameterName>@indicadorRetorno</tns:ParameterName>
						<tns:ParameterType>VARCHAR</tns:ParameterType>
						<tns:ParameterScale>1</tns:ParameterScale>
					</tns:Parameter>
				</tns:Parameters>
		</tns:MetadataQuery>
<!-- Cunsulta el nombre de la ciudad ,los parametros de ruta y de funcionario -->
		<tns:MetadataQuery>
			<tns:QueryName>SP_ParametroCertificado</tns:QueryName>
			<tns:CommandText>BEPS_TRANSACCIONAL.vinculacion.pr_vinc_consultarCertificadoParametrico(?,?,?)</tns:CommandText>
