						<tns:ParameterScale>10</tns:ParameterScale>
					</tns:Parameter>
					<tns:Parameter>
						<tns:ParameterName>@sistemaOrigen</tns:ParameterName>
						<tns:ParameterType>VARCHAR</tns:ParameterType>
						<tns:ParameterScale>50</tns:ParameterScale>
					</tns:Parameter>			
					<tns:Parameter>
						<tns:ParameterName>@indicadorRetorno</tns:ParameterName>
						<tns:ParameterType>VARCHAR</tns:ParameterType>
						<tns:ParameterScale>1</tns:ParameterScale>
					</tns:Parameter>
				</tns:Parameters>
		</tns:MetadataQuery>
		
